                                                   {if $property.info.pay_status == 'complete'  }
                                                        <button id="remove-button-{$property.info.property_id}" class="btn-remove f-right btn-view-search-list" style="margin-top:20px;margin-right: 28px" onclick="showMess('You can not remove this property !')"></button>
